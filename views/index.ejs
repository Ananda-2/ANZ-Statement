    <transaction>

      <div class="transaction-container">
        <div class="statement-header">
            <h1 class="statement-title">ANZ ACCESS ADVANTAGE STATEMENT</h1>
            <h3 class="account-number">Account Number 123456789</h3>
        </div>

        <div class="transaction-section">
            <h2>Transaction Details</h2>
            <div class="tax-notice">Please retain this statement for taxation purposes</div>

            <table class="transactions-table">
                <thead>
                    <tr>
                        <th class="amount-column-left">Date</th>
                        <th class="amount-column-left" >Transaction Details</th>
                        <th class="amount-column">Withdrawals ($)</th>
                        <th class="amount-column">Deposits ($)</th>
                        <th class="amount-column">Balance ($)</th>
                    </tr>
                </thead>
                <tbody>
                    <% transactions.forEach(function(transaction) { %>
                        <tr <%= transaction.isYear ? 'class="year-marker"' : '' %>>
                            <td class="date-column">
                                <% if (transaction.isYear) { %>
                                    <%= transaction.year %>
                                <% } %>
                                <div class="">
                                  <%= transaction.date %>
                                </div>
                            </td>
                            <td class="transaction-details">
                                <div class="bold-transaction-details"><%= transaction.details %></div>
                                <% if (transaction.description) { %>
                                    <div class="transaction-description"><%= transaction.description %></div>
                                <% } %>
                            </td>
                            <td class="amount-column"><%= transaction.withdrawal || '' %></td>
                            <td class="amount-column"><%= transaction.deposit || '' %></td>
                            <td class="balance-column"><%= transaction.balance %></td>
                        </tr>
                    <% }); %>
                </tbody>
            </table>
        </div>
    </div>
    </transaction>



